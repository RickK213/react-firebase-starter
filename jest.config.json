{
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/index.js",
    "!src/components/root/root.js"
  ],
  "coverageReporters": [
    "lcov",
    "text-summary"
  ],
  "reporters": ["jest-dot-reporter"],
  "setupFiles": [
    "./test-setup.js"
  ],
  "testMatch": [
    "**/?(*.)+(test).js"
  ],
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}