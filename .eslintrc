{
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "rules": {
    "comma-dangle": 0,
    "import/prefer-default-export": [0],
    "prettier/prettier": 1,
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "react/jsx-filename-extension": [0],
    "react/static-property-placement": [0],
    "react/jsx-props-no-spreading": [0],
    "react/forbid-prop-types": [0],
    "react/jsx-fragments": [0]
  }
}