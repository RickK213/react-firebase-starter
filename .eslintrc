{
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "import/prefer-default-export": [0],
    "prettier/prettier": 1,
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}